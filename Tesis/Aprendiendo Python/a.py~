#!/usr/bin/python

print 'Hola Mundo'

x=5
y=3

if (x > y):
   print("x es mayor que y")
else:
   print("y es menor que x")
   
raw_input() # Espera enter para seguir

c = [[0,1,2],[3,4,5]]
print c[0]     # fila 0
print c[1]     # fila 1
print c        # matriz
print c[1][2]  # imprime la ubicacion 0,1
d = [[0,1,2],[3,4,5]]

f = float(c[1][2]) / float(d[1][2])
print f


for i in range(1, 5, 2):# desde, hasta, i+2
	print i

	
b=100
a=10
while (b > a):
   c = b/3.0
   b = b-30
   print c	
   